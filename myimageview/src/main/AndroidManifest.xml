<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uniquedu.myimageview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NormalActivity"></activity>
        <activity android:name=".MatrixActivity"></activity>
        <activity android:name=".FitXYActivity"></activity>
        <activity android:name=".FitStartActivity"></activity>
        <activity android:name=".FitCenterActivity"></activity>
        <activity android:name=".FitEndActivity"></activity>
        <activity android:name=".CenterActivity"></activity>
        <activity android:name=".CenterCropActivity"></activity>
        <activity android:name=".CenterInsideActivity"></activity>
        <activity android:name=".ChangeImageActivity"></activity>
    </application>

</manifest>
